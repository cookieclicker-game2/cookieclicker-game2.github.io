        <script>
            function showToast(message) {
                var c = document.getElementById('toast-container');
                if (!c) return;
                var t = document.createElement('div');
                t.className = 'toast';
                t.innerHTML = '<i>‚úì</i> <span>' + message + '</span>';
                c.appendChild(t);
                requestAnimationFrame(function () { t.classList.add('show'); });
                setTimeout(function () { t.classList.remove('show'); setTimeout(function () { t.remove(); }, 300); }, 3000);
            }
            function toggleFavorite() {
                var btn = document.querySelector('.action-btn[title="Favorite"]');
                if (btn) { btn.classList.toggle('active'); showToast(btn.classList.contains('active') ? 'Added to favorites' : 'Removed from favorites'); }
            }
            function shareGame() {
                navigator.clipboard.writeText(window.location.href).then(function () { showToast('Link copied to clipboard'); }).catch(function () { showToast('Failed to copy link'); });
            }
            function toggleTheaterMode() {
                var hero = document.querySelector('.game-hero');
                if (hero) { hero.classList.toggle('theater-mode'); if (hero.classList.contains('theater-mode')) hero.scrollIntoView({ behavior: 'smooth' }); showToast(hero.classList.contains('theater-mode') ? 'Theater mode enabled' : 'Theater mode disabled'); }
            }
        </script>
