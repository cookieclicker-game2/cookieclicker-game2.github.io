    <script>
        window.toggleFullscreen = function () {
            var e = document.getElementById('game-frame');
            if (e && e.requestFullscreen) e.requestFullscreen();
            else if (e && e.webkitRequestFullscreen) e.webkitRequestFullscreen();
            else if (e && e.msRequestFullscreen) e.msRequestFullscreen();
            if (typeof showToast === 'function') showToast('Entered fullscreen');
        };
    </script>
